option solver minos;
var p1>=0, <=1;
var p2>=0, <=1;
var p3>=0, <=1;
var p4>=0, <=1;
minimize obj: -0.16324100902823002*p4+-0.23713901184069364*p3+-0.0019787339745730437*p2+0.9530925295567135*p1;
subject to r1:  p1-p2<=0.1;
subject to r2:  p2-p1<=0.1;
subject to r3:  p4-p3<=0.1;
subject to r4:  p3-p4<=0.1;
subject to r5:  1-p3>=0.4;
subject to r6:  1-p4>=0.4;
subject to r7:  1-p3<=0.6;
subject to r8:  1-p4<=0.6;
solve;
print 'p1',p1;
print 'p2',p2;
print 'p3',p3;
print 'p4',p4;
